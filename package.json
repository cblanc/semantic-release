{
  "name": "@cablanchard/semantic-release",
  "description": "semantic-release configuration",
  "repository": {
    "type": "git",
    "url": "https://github.com/cblanc/semantic-release.git"
  },
  "bugs": {
    "url": "https://github.com/cblanc/semantic-release/issues/new"
  },
  "version": "1.3.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json lib/**/*.ts",
    "build": "npm run lint && tsc",
    "semantic-release": "semantic-release",
    "watch": "mocha --watch-extensions ts --watch"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "engines": {
    "node": ">=8.0"
  },
  "author": {
    "name": "Christopher Blanchard",
    "email": "git@nf3.co",
    "url": "nf3.co"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "release": {
    "extends": "./dist/npm"
  },
  "devDependencies": {
    "@cablanchard/tsconfig": "~2.0.0",
    "@cablanchard/tslint": "0.0.1",
    "prettier": "~3.3.2",
    "semantic-release": "~24.0.0",
    "source-map-support": "~0.5.21",
    "ts-node": "~10.9.2",
    "tslint": "~6.1.2",
    "typescript": "~5.5.3"
  },
  "dependencies": {
    "@semantic-release/changelog": "~6.0.3",
    "@semantic-release/commit-analyzer": "~13.0.0",
    "@semantic-release/git": "~10.0.1",
    "@semantic-release/github": "~10.1.0",
    "@semantic-release/npm": "~12.0.1",
    "@semantic-release/release-notes-generator": "~14.0.1"
  }
}
